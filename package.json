{
  "name": "gen-pr",
  "version": "0.0.0-semantically-released",
  "private": true,
  "repository": "github:WillBooster/gen-pr",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "bin": "bin/gen-pr.js",
  "files": [
    "bin/gen-pr.js",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts app --input src/action.ts --input src/cli.ts",
    "check-all-for-ai": "bun run check-for-ai && bun run test --silent",
    "check-for-ai": "bun install > /dev/null && bun run cleanup && bun run typecheck",
    "cleanup": "bun --bun wb lint --fix --format",
    "format": "bun --bun wb lint --format",
    "lint": "bun --bun wb lint",
    "lint-fix": "bun --bun wb lint --fix",
    "prepare": "lefthook install || true",
    "release": "bun semantic-release && git tag -f v2 main && git push origin v2 --force",
    "start": "bun --env-file=.env src/cli.ts",
    "test": "bun wb test",
    "test-local": "bun --env-file=.env test test/unit-local",
    "typecheck": "bun --bun wb typecheck"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {},
  "devDependencies": {
    "@actions/core": "1.11.1",
    "@ai-sdk/amazon-bedrock": "3.0.0-canary.19",
    "@ai-sdk/anthropic": "2.0.0-canary.19",
    "@ai-sdk/azure": "2.0.0-canary.21",
    "@ai-sdk/google": "2.0.0-canary.20",
    "@ai-sdk/google-vertex": "3.0.0-canary.20",
    "@ai-sdk/openai": "2.0.0-canary.20",
    "@biomejs/biome": "2.0.6",
    "@types/bun": "1.2.17",
    "@types/micromatch": "4.0.9",
    "@types/yargs": "17.0.33",
    "@willbooster/biome-config": "3.1.4",
    "@willbooster/prettier-config": "10.1.3",
    "@willbooster/wb": "8.1.16",
    "ai": "5.0.0-canary.24",
    "ansis": "4.1.0",
    "build-ts": "15.0.8",
    "conventional-changelog-conventionalcommits": "9.0.0",
    "lefthook": "1.11.14",
    "prettier": "3.6.2",
    "semantic-release": "24.2.5",
    "sort-package-json": "3.3.1",
    "typescript": "5.8.3",
    "undici": "7.11.0",
    "yaml": "2.8.0",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">=22"
  }
}